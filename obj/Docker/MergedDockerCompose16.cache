networks:
  local:
    driver: bridge
services:
  ded.api:
    build:
      context: C:\Users\SISTEMAS_27\source\DED
      dockerfile: src/DED.API/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: DED.API
      target: base
    container_name: api
    depends_on:
      mongodb:
        condition: service_started
      rabbitmq:
        condition: service_started
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: dedapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "/app/bin/Debug/net5.0/DED.API.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      local: {}
    ports:
    - published: 44444
      target: 80
    - published: 44445
      target: 443
    restart: on-failure:10
    tty: true
    volumes:
    - C:\Users\SISTEMAS_27\source\DED\src\DED.API:/app:rw
    - C:\Users\SISTEMAS_27\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\SISTEMAS_27\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\SISTEMAS_27\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Program Files (x86)\Microsoft\Xamarin\NuGet:/root/.nuget/fallbackpackages:ro
    - C:\Users\SISTEMAS_27\.nuget\packages:/root/.nuget/packages:ro
    - C:\Users\SISTEMAS_27\source\DED:/src:rw
  ded.devicesbus:
    build:
      context: C:\Users\SISTEMAS_27\source\DED
      dockerfile: src/DED.DevicesBus/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: DED.DevicesBus
      target: base
    container_name: devicebus
    depends_on:
      mongodb:
        condition: service_started
      rabbitmq:
        condition: service_started
    entrypoint: tail -f /dev/null
    environment:
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: devicesbus:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "/app/bin/Debug/net5.0/DED.DevicesBus.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      local: {}
    restart: on-failure:10
    tty: true
    volumes:
    - C:\Users\SISTEMAS_27\source\DED\src\DED.DevicesBus:/app:rw
    - C:\Users\SISTEMAS_27\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Program Files (x86)\Microsoft\Xamarin\NuGet:/root/.nuget/fallbackpackages:ro
    - C:\Users\SISTEMAS_27\.nuget\packages:/root/.nuget/packages:ro
  ded.web:
    build:
      context: C:\Users\SISTEMAS_27\source\DED
      dockerfile: src/DED.Web/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: DED.Web
      target: base
    container_name: web
    depends_on:
      ded.api:
        condition: service_started
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: dedweb:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "/app/bin/Debug/net5.0/DED.Web.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      local: {}
    ports:
    - published: 44446
      target: 80
    - published: 44447
      target: 443
    restart: on-failure:10
    tty: true
    volumes:
    - C:\Users\SISTEMAS_27\source\DED\src\DED.Web:/app:rw
    - C:\Users\SISTEMAS_27\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\SISTEMAS_27\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\SISTEMAS_27\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Program Files (x86)\Microsoft\Xamarin\NuGet:/root/.nuget/fallbackpackages:ro
    - C:\Users\SISTEMAS_27\.nuget\packages:/root/.nuget/packages:ro
    - C:\Users\SISTEMAS_27\source\DED:/src:rw
  mongodb:
    container_name: mongo
    image: mongo
    networks:
      local: {}
    ports:
    - published: 27017
      target: 27017
    restart: unless-stopped
  rabbitmq:
    container_name: rabbit
    healthcheck:
      interval: 10s
      retries: 5
      test:
      - CMD-SHELL
      - "if rabbitmqctl status; then \nexit 0 \nfi \nexit 1"
    image: rabbitmq:3.8-management
    networks:
      local: {}
    ports:
    - published: 5672
      target: 5672
    - published: 15672
      target: 15672
    restart: on-failure:10
version: '3.4'